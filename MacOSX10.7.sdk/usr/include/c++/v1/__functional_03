template<class _F, class _Alloc, class _R, class _A0, class _A1>
_R
__func<_F, _Alloc, _R(_A0, _A1)>::operator()(_A0 __a0, _A1 __a1)
{
    return __invoke(__f_.first(), __a0, __a1);
}

#ifndef _LIBCPP_NO_RTTI

template<class _F, class _Alloc, class _R, class _A0, class _A1>
